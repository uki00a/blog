---
import ArticleListLayout from '../layouts/ArticleList.astro';

let allArticles = Astro.fetchContent('./articles/*.md');
allArticles = allArticles.sort((a, b) => new Date(b.publishDate) - new Date(a.publishDate));
---
<ArticleListLayout
  title="TOP"
  description="uki00a.github.io"
  articles={allArticles}
/>
